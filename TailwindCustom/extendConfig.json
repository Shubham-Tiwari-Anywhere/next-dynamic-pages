{
  "fontFamily": {
    "borna": ["var(--font-borna)"]
  },
  "transitionTimingFunction": {
    "ease-in": "cubic-bezier(0.895, 0.025, 0.685, 0.25)",
    "ease-out": "cubic-bezier(0.165, 0.85, 0.45, 1)",
    "ease-in-out": "cubic-bezier(0.75, 0, 0.175, 1)",
    "ease-out-back": "cubic-bezier(0.175, 0.885, 0.320, 1.275)",
    "custom": "cubic-bezier(0.5, 0, 0.1, 1)"
  },
  "transitionDuration": {
    "fast": "0.2s",
    "DEFAULT": "0.4s",
    "slow": "0.8s"
  },
  "keyframes": {
    "pulse-diffusion": {
      "0%": {
        "box-shadow": "0 0"
      },
      "70%": {
        "box-shadow": "0 0 8px 12px rgba(255, 255, 255, 0)"
      },
      "100%": {
        "box-shadow": "0 0 0 0 rgba(255, 255, 255, 0)"
      }
    },
    "loading": {
      "0%": {
        "width": "0"
      },
      "100%": {
        "width": "100%"
      }
    }
  },
  "animation": {
    "pulse-diffusion": "pulse-diffusion 3s infinite backwards",
    "loading": "loading 3s forwards"
  }
}
